#endif"

ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS VERSION LTI_BASEDIR ifGNUmake CXX CXXFLAGS LDFLAGS CPPFLAGS ac_ct_CXX EXEEXT OBJEXT INSTALL_PROGRAM INSTALL_SCRIPT INSTALL_DATA F77 FFLAGS ac_ct_F77 CC CFLAGS ac_ct_CC CPP EGREP F2C_LIBS FLIBS BLAS_LIBS LAPACK_LIBS EXTRAINCLUDEPATH EXTRALIBPATH EXTRALIBS EXTRACXXFLAGS JPEGLIB PNGLIB CXXCPP X_CFLAGS X_PRE_LIBS X_LIBS X_EXTRA_LIBS GTK_CONFIG gtk_lib GTK_CFLAGS GTK_LIBS OSCXXOPTS _ac_cxx_major _ac_cxx_minor _ac_cpuarch_gcc2 _ac_cpuarch_gcc3x _ac_cpuarchd_gcc2 _ac_cpuarchd_gcc3x LIB_DEBUG LIB_RELEASE LIBOBJS LTLIBOBJS'
ac_subst_files=''

# Initialize some variables set by options.
ac_init_help=
ac_init_version=false
# The variables have the same names as the options, with
# dashes changed to underlines.
cache_file=/dev/null
